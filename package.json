  "devDependencies": {
    "@types/react": "^16.0.5",
    "@types/react-dom": "^16.8.2",
    "@types/react-test-renderer": "^16.8.1",
    "ava": "^1.3.1",
    "browser-env": "^3.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.1.0",
    "prop-types": "^15.5.10",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-test-renderer": "^16.8.4",
    "ts-loader": "^5.3.3",
    "tslint": "^5.7.0",
